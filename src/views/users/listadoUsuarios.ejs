<%- include('../partials/head') %>
<body>
    <%- include('../partials/header') %>
    <main>
        <section>
            <div>
                <br>
                <% if (locals.errores) { %>
                        <p class="error_msg_form"><i class="fa-solid fa-triangle-exclamation"></i> - <%= locals.errores .msg %></p>
                <% }%>
                <% if (locals.session) { %>
                     <p><%= locals.session %></p>
                <% }%>
                <!-- -->
                <ul>
                    <% for( let i=0; i < users.length; i++ ) { %>
                        <li><%= users[i].email %> </li>
                        <li><%= users[i].nombre %> </li>
                        <li><%= users[i].apellido %> </li>
                        
                    <% } %> 
                </ul>
            </div> 
            <br>
        </section>
    </main>

    <%- include('../partials/footer') %>

</body>
</html>
